{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-explicit-any": "error",
        "linebreak-style": 0,
        "import/extensions": [0, { "js": "always" }]
    }
    
}
